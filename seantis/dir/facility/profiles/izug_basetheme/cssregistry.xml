<?xml version="1.0"?>
<object name="portal_css" meta_type="Stylesheets Registry">

    <stylesheet
        id="++resource++seantis.dir.facility.css/izug/directory.css"
        expression="python:'seantis.dir.facility' in object.portal_type"
        media="all" rel="stylesheet" rendering="link"/>

    <stylesheet 
        id="++resource++seantis.dir.facility.css/izug/item.css" 
        expression="python:object.portal_type in ('seantis.dir.facility.item', 'seantis.reservation.resource')"
        media="all" rel="stylesheet" rendering="link"/>
    
</object>
